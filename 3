#include <stdio.h>
#include <math.h>

void main()
{
    float r, t, b, a;
    
    a = 6.347;
    b = 21.4;
    
    printf("r = ");
    scanf("%f", &r);
    
    if(r>=a){
        printf("First condition has been performed");
        t = (a*a + pow(sqrt(r),3))/pow(log(r+b), 2);
        printf("\nt = %f\n", t);
        
    }else{
        if(a/2>r && r>=a/3){ 
            printf("Second condition has been performed");
            t = sqrt(a*exp(r))/r;
            printf("\nt = %f\n", t);
           
        }else{
            printf("There is no result!!!");
            
        }
    }

}
